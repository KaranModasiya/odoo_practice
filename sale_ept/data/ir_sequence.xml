<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
	<record id="seq_crm_lead" model="ir.sequence">
		<field name="name">CRM Leads</field>
		<field name="code">crm.lead.ept</field>
		<field name="prefix">Lead/</field>
		<field name="padding">6</field>
		<field name="company_id" eval="False"/>
	</record>


	<record id="seq_sale_order" model="ir.sequence">
		<field name="name">Sale Orders</field>
		<field name="code">sale.order.ept</field>
		<field name="prefix">SO</field>
		<field name="padding">5</field>
		<field name="company_id" eval="False"/>
	</record>


	<record id="seq_stock_picking_in" model="ir.sequence">
		<field name="name">Stock Picking Incoming</field>
		<field name="code">stock.picking.ept.in</field>
		<field name="prefix">WH/IN/</field>
		<field name="padding">4</field>
		<field name="company_id" eval="False"/>
	</record>


	<record id="seq_stock_picking_out" model="ir.sequence">
		<field name="name">Stock Picking Incoming</field>
		<field name="code">stock.picking.ept.out</field>
		<field name="prefix">WH/OUT/</field>
		<field name="padding">4</field>
		<field name="company_id" eval="False"/>
	</record>


	<record id="seq_prchase_order" model="ir.sequence">
		<field name="name">Purchase Orders</field>
		<field name="code">purchase.order.ept</field>
		<field name="prefix">PO</field>
		<field name="padding">5</field>
		<field name="company_id" eval="False"/>
	</record>
</odoo>